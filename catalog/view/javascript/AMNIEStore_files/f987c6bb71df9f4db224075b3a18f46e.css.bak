    padding: 8px 10px 8px 5px;
    font-size: 0.9em;
}

header nav ul.nav-main ul.dropdown-menu li:last-child > a {
    border-bottom: 0;
}

header nav ul.nav-main li a {
    font-size: 12px;
    font-style: normal;
