@font-face {
    font-family: 'Lato';
    src: url('http://www.betronstore.co.uk/skin/frontend/mgstheme/dukan/mgs/deals/css/fonts/Lato.eot');
    src: local('?'), url('http://www.betronstore.co.uk/skin/frontend/mgstheme/dukan/mgs/deals/css/fonts/Lato.woff') format('woff'), 
        url('http://www.betronstore.co.uk/skin/frontend/mgstheme/dukan/mgs/deals/css/fonts/Lato.ttf') format('truetype'), 
